{
  "name": "idev",
  "version": "0.0.1",
  "description": "a mobile develop dev tool",
  "bin": {
    "idev": "./bin/cli"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf output webui/output",
    "build": "npm run clean && webpack --config webui/webpack.config.babel.js --colors",
    "pub": "cross-env NODE_ENV=production npm run build",
    "lint": "fecs -v && fecs ./*.js --type=js --rule",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atwxp/idev.git"
  },
  "port": 8888,
  "webpackPort": 8080,
  "ports": [
    "uiport"
  ],
  "host": "127.0.0.1",
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "keywords": [
    "dev tool",
    "deugger",
    "http proxy"
  ],
  "author": "wxp201013@163.com",
  "license": "ISC",
  "preferGlobal": true,
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "cross-env": "^3.1.2",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fecs-loader": "^0.3.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.1",
    "vue-loader": "^9.5.1",
    "vue-router": "^2.0.0",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "dependencies": {
    "babel-preset-stage-2": "^6.18.0",
    "body-parser": "^1.15.2",
    "commander": "^2.9.0",
    "express": "^4.14.0",
    "fs-extra": "^1.0.0",
    "http-proxy": "^1.15.1",
    "js-beautify": "^1.6.4",
    "json-formatter-js": "^1.2.0",
    "mime": "^1.3.4",
    "multiparty": "^4.1.2",
    "node-forge": "^0.6.45",
    "prismjs": "^1.5.1",
    "promise": "^7.1.1",
    "qrious": "^2.2.0",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.5.0",
    "v-qrcode": "^1.0.1",
    "vue": "^2.0.1",
    "vue-resource": "^1.0.3",
    "vue-select": "^1.3.3",
    "vuex": "^2.0.0"
  }
